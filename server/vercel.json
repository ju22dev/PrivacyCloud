{
    "version" : 2,
    "builds" : [{ "src" : "./index.js", "use" : "@vercel/node"}],
    "routes" : [
        { 
            "src" : "/(.*)", 
            "methods" : ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH"], 
            "dest" : "/",
            "headers" : {
                "Access-Control-Allow-Origin" : "*"
            }
        }
    ]
}